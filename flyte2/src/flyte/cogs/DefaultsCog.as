package flyte.cogs{	import flash.geom.Point;		import flyte.core.Base;		public class DefaultsCog extends BaseCog	{		public var position:Point		public var dimensions:Point		public function DefaultsCog(t:Base):void		{			this._target = t			capture()		}				public function capture():void		{			dimensions = new Point(target.x,target.y)			position = new Point(target.x,target.y)		}	}}